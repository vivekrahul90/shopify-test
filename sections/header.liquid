This file has been modified by the Yoast SEO for Shopify app.
You can learn more about this (and learn how to cleanly revert these changes) at https://yoa.st/removing-theme-changes.
{%- endcomment -%}
{%- liquid
  assign areas = "heading navigation icons"
  assign areas2 =  ". . ."
  assign class_grid = "col-span-full col-start-1 order-1 lg:mb-3"
  assign class = 'w-full justify-end'
  assign logo_above = false
  assign logo_center = false
  assign columns = 'auto 1fr auto'
  if section.settings.logo_position == 'above-center'
    assign areas =  "iconsRight heading icons" 
    assign areas2 =  "navigation navigation navigation"
    assign class_grid = '[grid-area:iconsRight] hidden lg:block'
    assign class = 'w-full'
    assign columns = '4fr 2fr 4fr'
    assign logo_above = true
  endif
  if section.settings.logo_position == 'left-center'
    assign columns = '1fr 4fr 1fr'
  endif
  if section.settings.logo_position == 'center-left'
    assign columns = '4fr 2fr 4fr'
    assign areas = ". . ."
    assign areas2 =  "navigation heading icons"
    assign logo_center = true
  endif
  assign transparent = false
  if section.settings.enable_on_homepage and template.name == "index"
    assign transparent = true
  endif
  if section.settings.enable_on_collection_page and template.name == "collection"
    assign transparent = true
  endif
  assign class_logo = "w-[" | append: section.settings.logo_width_mobile | append: "px] md:w-[" | append: section.settings.logo_width | append: "px] h-auto"
  assign logo_name = settings.heading_base_size | times: 0.0225
  if section.settings.header_show_currency_selector or section.settings.show_social_icons or section.settings.header_show_language_selector
    assign headerTop = true 
  else 
    assign headerTop = false
  endif
  if settings.lang_direction contains request.locale.iso_code
    assign is_rtl = true
    assign class_rtl = 'rtl'
  else
    assign is_rtl = false
    assign class_rtl = 'ltr'
  endif
-%}
{%- style -%}
  {% if section.settings.sticky_header_type == "none" or section.settings.sticky_header_type == "on-scroll-up" %}
    :root{
      --height-header: 0px;
    }
  {% endif %}
  {%- if transparent -%}
    section:first-child {
      margin-top: 0;
    }
    #sticky-header-content {
      background: linear-gradient(to top, transparent 0%, rgba({{ section.settings.shadow_color.red }},{{ section.settings.shadow_color.green }},{{ section.settings.shadow_color.blue }}, {{ section.settings.shadow }}%) 100%);
    }
    .dark #sticky-header-content {
      {% if section.settings.shadow_color_dark.alpha != 0.0 %}
        background: linear-gradient(to top, transparent 0%, rgba({{ section.settings.shadow_color_dark.red }},{{ section.settings.shadow_color_dark.green }},{{ section.settings.shadow_color_dark.blue }}, {{ section.settings.shadow }}%) 100%);
      {% else %}
        background: transparent;
      {% endif %}
    }
    #sticky-header-content.background-header {
      background: rgb(var(--background-color-header));
    }
    #sticky-header-content:hover {
      background: rgba(var(--background-color-header))
    }
    #sticky-header-content:hover .divider {
      -colors-line-and-border: var(--background-color-header);
      --colors-line-header: var(--background-color-header);
    }
    .transparent-header .sticky-header-content:not(.sticky-header-active) .header {
      {% unless section.settings.textlink_color_light.alpha == 0.0 %}
        --colors-text-link: {{ section.settings.textlink_color_light.red }},{{ section.settings.textlink_color_light.green }},{{ section.settings.textlink_color_light.blue }};
      {% endunless %}
    }
    .dark .sticky-header-content:not(.sticky-header-active) .header {
      {%- unless section.settings.textlink_color_dark.alpha == 0.0 -%}
        --colors-text-link: {{ section.settings.textlink_color_dark.red }},{{ section.settings.textlink_color_dark.green }},{{ section.settings.textlink_color_dark.blue }};
      {% else %}  
        --colors-text-link: {{ settings.colors_dark_text_link.red }}, {{ settings.colors_dark_text_link.green }}, {{ settings.colors_dark_text_link.blue }};
      {% endunless %}
    }
  {%- endif -%}
  .logo-name {
    font-size: {{ logo_name | times: 0.5 }}rem;
  }
  .transparent-header .logo-name,
  .transparent-header header {
    {% unless section.settings.text_color_light.alpha == 0.0 %}
      color: {{ section.settings.text_color_light }};
    {% endunless %}
  }
  .dark .transparent-header .logo-name,
  .dark .transparent-header header {
    {% unless section.settings.text_color_dark.alpha == 0.0 %}
      color: {{ section.settings.text_color_dark }};
    {% else %}
      color: rgba(var(--colors-text));
    {% endunless %}
    
  }
  {% if section.settings.sticky_header_type == "reduce-logo-size" %}
    .reduce-logo-size .header-logo {
      width: 75%;
    }
  {% endif %}
  @media (min-width: 1024px) {
    header {
      grid-template-areas: "{{ areas }}" "{{ areas2 }}";
      grid-template-columns: {{ columns }};
    } 
    .logo-name {
      font-size: {{ logo_name }}rem;
    }
    .pt-header {
      padding-top: {{ section.settings.padding_top }}px;
    }
    .pb-header {
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%} 
<div
  id="x-header-container"
  class="relative text-[rgb(var(--colors-text-header))] z-50 {{ class_rtl }}"
  x-data
  x-init='$store.xHeaderMenu.initSticky($el, "shopify-section-{{ section.id }}", "{{ section.settings.sticky_header_type }}")'
  {% if section.settings.sticky_header_type != "none" %}
    x-intersect:leave="$store.xHeaderMenu.handleAlwaysSticky()"
    data-is-sticky="true"
  {% endif %}
  @scroll.window="$store.xHeaderMenu.removeStickyHeader();{% if section.settings.sticky_header_type == 'on-scroll-up' %} $store.xHeaderMenu.handelOnScrollSticky(){% endif %}"
>
  <div id="sticky-header" class="shopify-header text-[rgb(var(--colors-text-header))]{% if transparent %} transparent-header{% else %} background-header{% endif %}"
    {% if section.settings.sticky_header_type == 'always' or section.settings.sticky_header_type == 'reduce-logo-size' %}x-intersect="$store.xHeaderMenu.setVariableHeightHeader(true)"{% endif %}
  >
    <div
      id="sticky-header-content"
      class="sticky-header-content z-20 w-full pt-[{{ section.settings.padding_top_mobile }}px] pb-[{{ section.settings.padding_bottom_mobile }}px] pt-header top-0 lg:pb-0{% if transparent %} absolute disable-tranparent-collection hover:bg-[rgba(var(--background-color-header))]{% endif %}
      {% if section.settings.full_width %} header-full-width{% endif %}"
      :class="{ 'background-header sticky-header-active': $store.xHeaderMenu.isSticky, '{% unless headerTop %} lg:pt-5{% endunless %}': !$store.xHeaderMenu.isSticky, 'overflow-hidden padding-scrollbar': $store.xHeaderMenu.isSticky && $store.xPopup.open}"
      {% if transparent %}
        @click="$store.xHeaderMenu.clickedHeader = true"
        @click.away="$store.xHeaderMenu.clickedHeader = false"
        x-on:mouseover="$store.xHeaderMenu.addTransparentHover($el); $store.xHeaderMenu.showLogoTransparent = false"
        @mouseleave="!$store.xHeaderMenu.isSticky && $store.xHeaderMenu.removeTransparentHover($el); $store.xHeaderMenu.showLogoTransparent=true"
      {% endif %}
      x-intersect.once="$store.xHeaderMenu.setTopStickyHeader()"
    >
      <header class="header{% if section.settings.full_width %} full-width{% else %} page-width{% endif %} flex md:grid lg:mx-auto gap-x-1{% if logo_above %} items-center{% endif %}" :class="{ 'lg:pb-0': $store.xHeaderMenu.isSticky }">
        <div data-breakpoint="tablet" class="[grid-area:drawer] lg:hidden flex items-center md:w-32">
          <div 
            @click="$store.xMobileNav.open()" 
            onclick="clevertap.event.push('Hamburger_click', {'Page name': '{% if request.page_type == 'index' %}Home page{% else %}{{ request.page_type }}{% endif %}', 'Page URL': '{% if request.page_type == 'index' %}{{ request.origin }}{% else %}{{ request.path }}{% endif %}'})" 
            class="relative {% if is_rtl %} -right-3{% else %} -left-3{% endif %} w-8 h-8 pl-3 pt-2.5 pb-2.5 rounded-full cursor-pointer opacity-70">        
            {% render 'icon-alls', icon: 'icon-hamburger' %}
          </div>
        </div>
        
        {%- if section.settings.header_show_language_selector or section.settings.show_social_icons or section.settings.header_show_currency_selector -%}
          <div class="{{ class_grid }} {{ class }} lg:gap-3" :class="{ 'max-h-0 opacity-0 lg:mb-0 overflow-hidden': $store.xHeaderMenu.isSticky }">
            <div class="hidden relative lg:block">
              {% render 'toolbar', 
                show_currency_selector: section.settings.header_show_currency_selector, 
                show_language_selector: section.settings.header_show_language_selector, 
                show_social_icons: section.settings.show_social_icons,
                logo_above: logo_above,
                header: true
              %}
            </div>
          </div>
        {%- endif -%} 

        {% if request.page_type == 'index' %}
          <h1 class="flex logo-name p-break-words lg:order-2 top-4 justify-center text-center [grid-area:heading] mx-auto{% if logo_above == false %} pb-header flex items-center lg:justify-left lg:relative lg:left-0 lg:translate-x-0 lg:mt-0 lg:top-0{% unless logo_center %} lg:[grid-area:auto] lg:ml-0{% endunless %}{% if is_rtl and logo_center == false %} lg:w-full{% endif %}{% else %} lg:mx-auto lg:pb-0{% endif %}">
        {% else %}
          <div class="logo-name p-break-words lg:order-2 top-4 justify-center text-center [grid-area:heading] mx-auto{% if logo_above == false %} pb-header flex items-center lg:justify-left lg:relative lg:left-0 lg:translate-x-0 lg:mt-0 lg:top-0{% unless logo_center %} lg:[grid-area:auto] lg:ml-0{% endunless %}{% if is_rtl and logo_center == false %} lg:w-full{% endif %}{% else %} lg:mx-auto lg:pb-0{% endif %}">
        {% endif %}
          {% if section.settings.logo_transparent and transparent %}
            {%- render 'header-logo-transparent', class_logo: class_logo, logo_above: logo_above, transparent: transparent, logo_transparent: section.settings.logo_transparent -%}
          {% else %}
            {%- render 'header-logo-block', class_logo: class_logo, logo_above: logo_above, transparent: transparent -%}
          {% endif %}
        {%- if request.page_type == 'index' -%}
          </h1>
        {% else %}
          </div>
        {%- endif -%}

        <div
          class="flex justify-end order-4 [grid-area:icons]{% if logo_above %} lg:[grid-area:icons]{% else %} {% if logo_center %} lg:[grid-area:icons]{% else %} lg:[grid-area:auto]{% endif %} pb-header{% endif %}"
        >
          {%- if settings.search_enabled -%}
            {%- render 'header-search-block' -%}
          {%- endif -%}
          {%- if shop.customer_accounts_enabled -%}
            <a href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"              
              aria-label="{{ 'customer.log_in' | t }}"
              class="header-icon-account hidden lg:flex opacity-70 relative items-center justify-center">
              <span class="w-11 h-10 pl-3 pr-2.5 pt-2.5 pb-2">
                {% render 'icon-alls', icon: 'icon-account' %}
              </span>
            </a>
          {%- endif -%}
          {%- unless settings.hide_cart -%}
            <div class="header-icon-cart cursor-pointer relative flex items-center">
              <a 
                id="cart-icon"
                onclick="clevertap.event.push('Cart_icon_click', {'Page name': '{% if request.page_type == 'index' %}Home page{% else %}{{ request.page_type }}{% endif %}', 'Page URL': '{% if request.page_type == 'index' %}{{ request.origin }}{% else %}{{ request.path }}{% endif %}'})"
                href="{{ routes.cart_url }}"
                class="relative flex items-center"              
                @click.prevent="$store.xModal.setActiveElement('cart-icon'); $store.xMiniCart.openCart();"
                :class="$store.xMiniCart.open && 'pointer-events-none'"
                aria-controls="CartDrawer"
                aria-expanded="false"
                aria-label="icon-cart"
              >
                <span class="w-8 md:w-11 h-10 pl-3 md:pl-0 inline-block md:pr-2.5 pt-2.5 pb-2 opacity-70">
                  {% render 'icon-cart-all', icon: settings.cart_icon %}
                </span>
                <div id="cart-icon-bubble" class="count_item absolute -top-0.5 {% if is_rtl %} left-1{% else %} left-6 md:left-auto md:right-0{% endif %}" data-value="{{ cart.item_count }}">
                  <div class="bg-[rgba(var(--colors-cart))] text-[rgba(var(--colors-cart-text))] font-medium h-5 w-5 -mr-1 rounded-full text-[10px]{% if cart == empty %} hidden{% endif %}">
                    {%- if cart.item_count < 100 -%}
                      <span class="h-full flex items-center justify-center mt-[1px]" aria-hidden="true">{{ cart.item_count }}</span>
                    {%- else -%}
                      <span class="h-full flex items-center justify-center mt-[1px]">99+</span> 
                    {%- endif -%}
                  </div>
                </div>
              </a>
            </div>
          {%- endunless -%}
          {%- if settings.theme_mode_enabled -%}
            {%- if settings.theme_mode_mode == 'switch' -%}
              <theme-mode x-data class="w-10 flex items-center justify-end lg:justify-center cursor-pointer{% if is_rtl %} mr-2{% else %} ml-2{% endif %}">
                <button
                  @click.prevent="$store.xDarkMode.toggleThemeMode(); $store.xHeaderMenu.handleChangeThemeMode();"
                  class="button button-action rounded-full text-sm pt-2.5 pb-2.5 pl-2.5 pr-2.5"
                  aria-label="darkmode"
                >
                  <span class="w-4 lg:w-5 hidden dark:block">
                    <svg
                      class="svg-icon"
                      style="vertical-align: middle;fill: currentColor;overflow: hidden;"
                      viewBox="0 0 1024 1024"
                      version="1.1"
                      xmlns="http://www.w3.org/2000/svg"
                    >
                      <path d="M557.250088 959.028243c-247.490697 0-448.826192-199.306278-448.826192-446.783673 0-244.284679 198.73118-446.636317 443.002556-446.636317 0.119727 0 0.226151 0 0.346901 0 10.354835 0 19.840884 3.807722 24.477484 13.094226 4.688789 9.379625 3.593851 19.61371-2.805906 27.92398-53.604872 69.477374-81.928964 152.101163-81.928964 239.895719 0 217.308281 176.792519 393.82553 394.1008 393.82553l5.051039-0.239454c9.927093-0.134053 20.175505 5.638417 24.864294 15.043624 4.704139 9.379625 3.621481 20.603247-2.804882 28.91454C826.947554 895.269104 697.373453 959.028243 557.250088 959.028243zM497.340627 120.789017c-188.636265 29.113061-334.191338 193.506179-334.191338 389.410987 0 217.294978 176.792519 394.1008 394.1008 394.1008 104.550171 0 202.412013-40.456411 275.696086-112.540146-222.772736-26.1598-396.157645-216.07929-396.157645-445.747063C436.787506 265.754666 457.578018 188.916651 497.340627 120.789017z"  /><path d="M577.719282 865.501126c-179.75806 0-332.640006-128.359436-363.490641-305.211307-1.296529-7.441483 3.687996-14.536065 11.129478-15.833617 7.54893-1.2689 14.537088 3.687996 15.832594 11.130502 28.55229 163.705456 170.09191 282.536888 336.52857 282.536888 7.563256 0 13.682627 6.119371 13.682627 13.681604C591.40191 859.381755 585.281515 865.501126 577.719282 865.501126z"  /><path d="M222.886835 530.788925c-7.255241 0-13.307074-6.734378-13.654998-14.055111-0.306992-6.333242-0.467651-13.267165-0.467651-19.694551 0-7.562233 6.118348-13.935384 13.681604-13.935384s13.681604 5.998621 13.681604 13.560854c0 5.998621 0.160659 12.920264 0.441045 18.839067 0.361227 7.54893-5.464455 15.285125-13.013385 15.285125C223.326856 530.788925 223.112985 530.788925 222.886835 530.788925z"  /><path d="M642.680806 310.140573c4.300956 12.746302-2.566452 26.587542-15.311731 30.890544-12.747325 4.288676-26.588565-2.565429-30.891568-15.325034-4.275373-12.746302 2.565429-26.575262 15.311731-30.877241C624.56317 290.525839 638.377804 297.394271 642.680806 310.140573z"  /><path d="M801.146934 193.09788c4.275373 12.746302-2.565429 26.588565-15.337313 30.890544-12.747325 4.275373-26.561959-2.565429-30.864962-15.325034-4.300956-12.746302 2.566452-26.574239 15.311731-30.877241C783.003715 173.48417 796.844955 180.351578 801.146934 193.09788z"  /><path d="M748.131487 411.503145c4.301979 12.759605-2.565429 26.588565-15.311731 30.890544-12.746302 4.288676-26.587542-2.565429-30.891568-15.311731-4.300956-12.746302 2.566452-26.587542 15.311731-30.890544C729.987245 391.889435 743.829508 398.756843 748.131487 411.503145z"  />
                    </svg>
                  </span>
                  <span class="block w-4 lg:w-5 dark:hidden">
                    <svg class="svg-icon" style="fill: currentColor;overflow: hidden;"  viewBox="0 0 1024 1024"  version="1.1" xmlns="http://www.w3.org/2000/svg">
                      <path d="M512 704a192 192 0 1 0 0-384 192 192 0 0 0 0 384z m0 64a256 256 0 1 1 0-512 256 256 0 0 1 0 512zM32 480h128a32 32 0 1 1 0 64H32a32 32 0 1 1 0-64z m832 0h128a32 32 0 1 1 0 64H864a32 32 0 1 1 0-64zM480 992V864a32 32 0 1 1 64 0v128a32 32 0 1 1-64 0z m0-832V32a32 32 0 1 1 64 0v128a32 32 0 1 1-64 0z m348.8 714.027l-90.539-90.496a32 32 0 1 1 45.27-45.227l90.496 90.453a32 32 0 0 1-45.227 45.27zM240.47 285.739L149.972 195.2a32 32 0 1 1 45.227-45.227l90.539 90.496a32 32 0 1 1-45.27 45.227z m633.6-90.539l-90.54 90.539a32 32 0 1 1-45.226-45.27l90.453-90.496a32 32 0 0 1 45.227 45.227zM285.652 783.53L195.2 874.028a32 32 0 0 1-45.27-45.227l90.497-90.539a32 32 0 1 1 45.226 45.27z"  />
                    </svg>
                  </span>
                </button>
              </theme-mode>
            {%- endif -%}
          {%- endif -%}
        </div>

        {%- render 'header-megamenu-block', logo_above: logo_above, logo_center: logo_center -%}

      </header>
      {%- liquid 
        unless template == 'cart'
          render 'cart-drawer', transparent: transparent
        endunless
      -%}
      {% render 'section-divider', classHeader: ' absolute top-full left-0 w-full' %}
    </div>
  </div>
</div>
<script src="{{ 'preview-color-swatches.js' | asset_url }}" defer></script>
{%- comment -%}Yoast SEO schema control block: start.{%- endcomment -%}
{%- assign yoast_seo_settings = shop.metafields.yoast_seo.settings.value | default: shop.metafields.yoast_seo.settings -%}
{%- if disabled_by_yoast_seo or yoast_seo_settings.schema.useTheme.schema or yoast_seo_settings.schema.useTheme.organization -%}
<script type="application/ld+json">
  {
    "@context": "http://schema.org",
    "@type": "Organization",
    "name": {{ shop.name | json }},
    {% if section.settings.logo %}
      "logo": {{ section.settings.logo | image_url: width: section.settings.logo.width | prepend: "https:" | json }},
    {% endif %}
    "sameAs": [
      {{ settings.social_twitter_link | json }},
      {{ settings.social_facebook_link | json }},
      {{ settings.social_pinterest_link | json }},
      {{ settings.social_instagram_link | json }},
      {{ settings.social_tiktok_link | json }},
      {{ settings.social_tumblr_link | json }},
      {{ settings.social_snapchat_link | json }},
      {{ settings.social_youtube_link | json }},
      {{ settings.social_vimeo_link | json }}
    ],
    "url": {{ request.origin | append: page.url | json }}
  }
{%- comment -%}Yoast SEO schema control block: end.{%- endcomment -%}
</script>
{%- endif -%}
{%- if request.page_type == 'index' -%}
  {% assign potential_action_target = request.origin | append: routes.search_url | append: "?q={search_term_string}" %}
  {%- comment -%}Yoast SEO schema control block: start.{%- endcomment -%}
{%- assign yoast_seo_settings = shop.metafields.yoast_seo.settings.value | default: shop.metafields.yoast_seo.settings -%}
{%- if disabled_by_yoast_seo or yoast_seo_settings.schema.useTheme.schema or yoast_seo_settings.schema.useTheme.website -%}
<script type="application/ld+json">
    {
      "@context": "http://schema.org",
      "@type": "WebSite",
      "name": {{ shop.name | json }},
      "potentialAction": {
        "@type": "SearchAction",
        "target": {{ potential_action_target | json }},
        "query-input": "required name=search_term_string"
      },
      "url": {{ request.origin | append: page.url | json }}
    }
  {%- comment -%}Yoast SEO schema control block: end.{%- endcomment -%}
</script>
{%- endif -%}
{%- endif -%}
{% schema %}
{
  "name": "t:sections.header.name",
  "class": "section-header",
  "settings": [
    {
      "type": "link_list",
      "id": "menu",
      "default": "main-menu",
      "label": "t:sections.header.settings.menu.label",
      "info": "t:sections.header.settings.menu.info"
    },
    {
      "type": "checkbox",
      "id": "full_width",
      "default": false,
      "label": "t:sections.text-column-image.settings.full_width.label"
    },
    {
      "type": "select",
      "id": "logo_position",
      "options": [
        {
          "value": "left-left",
          "label": "t:sections.header.settings.logo_position.options__1.label"
        },
        {
          "value": "left-center",
          "label": "t:sections.header.settings.logo_position.options__2.label"
        },
        {
          "value": "above-center",
          "label": "t:sections.header.settings.logo_position.options__3.label"
        },
        {
          "value": "center-left",
          "label": "t:sections.header.settings.logo_position.options__4.label"
        },
        {
          "value": "left-right",
          "label": "t:sections.header.settings.logo_position.options__5.label"
        }
      ],
      "default": "left-left",
      "label": "t:sections.header.settings.logo_position.label",
      "info": "t:sections.header.settings.logo_position.info"
    },
    {
      "type": "select",
      "id": "sticky_header_type",
      "options": [
        {
          "value": "none",
          "label": "t:sections.header.settings.sticky_header_type.options__1.label"
        },
        {
          "value": "on-scroll-up",
          "label": "t:sections.header.settings.sticky_header_type.options__2.label"
        },
        {
          "value": "always",
          "label": "t:sections.header.settings.sticky_header_type.options__3.label"
        },
        {
          "value": "reduce-logo-size",
          "label": "t:sections.header.settings.sticky_header_type.options__4.label"
        }
