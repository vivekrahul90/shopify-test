{%- liquid
  assign base_size_heading = settings.heading_base_size | times: 0.0225 
  assign font_size_heading = section.settings.title_size | times: 0.01 | times: base_size_heading
  assign thumbnail_desktop = false
  if section.settings.desktop_media_layout == 'thumbnail_slider'
    assign thumbnail_desktop = true
  endif 
  assign mobile_media_full = true
  if section.settings.mobile_media_size == '75%'
    assign mobile_media_full = false
  endif 
  assign product_form_id = 'product-form-' | append: section.id
-%}
{%- style -%}
  .content-{{ section.id }} {
    background: {{ settings.colors_background }};
  }
  .dark .content-{{ section.id }} {
    background: {{ settings.colors_dark_background }};
  }
  .title-size--{{ section.id }} {
    font-size: {{ font_size_heading | times: 0.60 }}rem;
  }
  @media (min-width: 767px){
    .title-size--{{ section.id }} {
      font-size: {{ font_size_heading }}rem;
    }
  }
{%- endstyle -%}
<div id="popup-fbt-{{ section.id }}"></div>
<div class="product-page main-product main-product-{{ section.id }}">
  <div id="breadcrumbs--{{ section.id }}">
    {% if settings.advanced_show_breadcrumbs and section.settings.show_breadcrumbs %}
      {% render 'breadcrumbs',
        background_color_light: section.settings.background_color_light,
        text_color_light: section.settings.text_color_light,
        textlink_color_light: section.settings.textlink_color_light,
        background_color_dark: section.settings.background_color_dark,
        text_color_dark: section.settings.text_color_dark,
        textlink_color_dark: section.settings.textlink_color_dark,
        desktop_content_alignment: section.settings.desktop_content_alignment,
        mobile_content_alignment: section.settings.mobile_content_alignment,
        full_width_breadcrumb: section.settings.full_width_breadcrumb,
        show_breadcrumbs_border: section.settings.show_breadcrumbs_border,
        show_image_banner: false
      %}
    {% endif %}
  </div>
  {%- 
    render 'product-template',
      product: product,
      product_form_id: product_form_id,
      is_product_page: true,
      enable_sticky_image: section.settings.enable_sticky_image,
      media_layout: section.settings.desktop_media_layout,
      show_thumbnail_desktop: thumbnail_desktop,
      show_arrow_button: section.settings.show_arrow_button,
      show_thumbnail_mobile: section.settings.show_thumnail_mobile,
      mobile_media_full: mobile_media_full,
      lazy_load: false,
      spacing_col: section.settings.spacing_col,
      full_width_main_page: section.settings.full_width,
      show_first_image_avaiable: section.settings.show_first_image_avaiable,
      enable_microdata: section.settings.enable_microdata
  -%}
</div>
<script src="{{ 'product-recommendations.js' | asset_url }}" defer></script>
<script src="{{ 'recently-viewed.js' | asset_url }}" defer></script>
<script src="{{ 'preview-color-swatches.js' | asset_url }}" defer></script>
