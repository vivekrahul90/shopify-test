                        <div id="cart-upsell-drawer-mob" class="cart-upsell-drawer-mob pl-5 pr-5 md:pl-0 md:pr-0 visible flex flex-row gap-x-2 lg:gap-x-0 overflow-x-auto lg:overflow-x-hidden scrollbar-body scrollbar-horizontal scrollbar-visible lg:grid lg:gap-5 lg:pb-2">
                          {%- liquid 
                            assign productCount = 0
                            assign productIds = cart.items | map: 'product_id' | uniq | join: ', ' | append: ","
                            assign maxItems = settings.number_of_product_shown
