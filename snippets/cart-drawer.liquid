                                              </a>
                                            {%- else -%}
                                              {% if property.first == name_edt and is_update_date %}
                                                {{ message_update }}
                                              {% else %}
                                                {{ property.last }}
                                              {% endif %}
                                            {%- endif -%}
                                          </dd>
