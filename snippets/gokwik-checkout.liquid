{% comment %} {% if settings.goEnable %}
  {% style %}
    .gokwik-checkout, .gokwik-checkout *:before {
      {% if settings.go_button_light.alpha != 0.0 %}
        --colors-button: {{ settings.go_button_light.red }}, {{ settings.go_button_light.green }}, {{ settings.go_button_light.blue }};
      {% endif %}
      {% if settings.go_button_text_light.alpha != 0.0 %}
        --colors-button-text: {{ settings.go_button_text_light.red }}, {{ settings.go_button_text_light.green }}, {{ settings.go_button_text_light.blue }};
      {% endif %}
      {% if settings.go_button_hover_light.alpha != 0.0 %}
        --colors-button-hover: rgb({{ settings.go_button_hover_light.red }}, {{ settings.go_button_hover_light.green }}, {{ settings.go_button_hover_light.blue }});
      {% endif %}
      {% if settings.go_button_text_hover_light.alpha != 0.0 %}
        --colors-button-text-hover: {{ settings.go_button_text_hover_light.red }}, {{ settings.go_button_text_hover_light.green }}, {{ settings.go_button_text_hover_light.blue }};
      {% endif %}
      {% if settings.go_secondary_button_text_light.alpha != 0.0 %}
        --colors-secondary-button: {{ settings.go_secondary_button_text_light.red }}, {{ settings.go_secondary_button_text_light.green }}, {{ settings.go_secondary_button_text_light.blue }};
        --colors-line-secondary-button: {{ settings.go_secondary_button_text_light.red }}, {{ settings.go_secondary_button_text_light.green }}, {{ settings.go_secondary_button_text_light.blue }};
        --background-secondary-button: transparent;
      {% endif %}
      {% if settings.go_secondary_button_light.alpha != 0.0 %}
        --background-secondary-button: {{ settings.go_secondary_button_light.red }}, {{ settings.go_secondary_button_light.green }}, {{ settings.go_secondary_button_light.blue }};
        --colors-line-secondary-button: {{ settings.go_secondary_button_light.red }}, {{ settings.go_secondary_button_light.green }}, {{ settings.go_secondary_button_light.blue }};
      {% endif %}
    }
  {% endstyle %}
<div class="gokwik-checkout"{% if settings.hideFlow %} style="display:none !important;" {% endif %}>
  <button type='button' class="button button-solid" 
     onclick="onCheckoutClick(this);clevertap.event.push('Cart_CTA_click')">
            <span class="btn-text" >
                <span>{{ settings.main-titile }}</span>
                <span>{{ settings.sub-titile }}</span>
            </span>
          <span class="pay-opt-icon">
            <svg xmlns="http://www.w3.org/2000/svg" version="1.1" xmlns:xlink="http://www.w3.org/1999/xlink" width="16" height="16" x="0" y="0" viewBox="0 0 492.004 492.004" style="enable-background:new 0 0 512 512" xml:space="preserve" class=""><g><path d="M484.14 226.886 306.46 49.202c-5.072-5.072-11.832-7.856-19.04-7.856-7.216 0-13.972 2.788-19.044 7.856l-16.132 16.136c-5.068 5.064-7.86 11.828-7.86 19.04 0 7.208 2.792 14.2 7.86 19.264L355.9 207.526H26.58C11.732 207.526 0 219.15 0 234.002v22.812c0 14.852 11.732 27.648 26.58 27.648h330.496L252.248 388.926c-5.068 5.072-7.86 11.652-7.86 18.864 0 7.204 2.792 13.88 7.86 18.948l16.132 16.084c5.072 5.072 11.828 7.836 19.044 7.836 7.208 0 13.968-2.8 19.04-7.872l177.68-177.68c5.084-5.088 7.88-11.88 7.86-19.1.016-7.244-2.776-14.04-7.864-19.12z" fill="#000000" opacity="1" data-original="#000000"></path></g></svg>
             <!-- <img src="https://cdn.gokwik.co/v4/images/upi-icons.svg">
             <img src="https://cdn.gokwik.co/v4/images/right-arrow.svg"> -->
         
 </span>
    <div style="display:none"><div class="cir-loader">Loading..</div></div>
  </button>
</div>
{% endif %}

 {% endcomment %}