{%- if settings.social_facebook_link != blank -%}
  <li>
    <a href="{{ settings.social_facebook_link }}" target="blank" aria-label="{{ 'general.social.links.facebook' | t }}" class="flex disable-effect hover-text-link w-11 h-11 pt-3 pr-3 pb-3 pl-3{% if footer %} border border-solid rounded-full{% endif %}">
