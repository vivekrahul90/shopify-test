{%- comment -%}
  This file has been modified by the Yoast SEO for Shopify app.
  You can learn more about this (and learn how to cleanly revert these changes) at https://yoa.st/removing-theme-changes.
{%- endcomment -%}
<!doctype html>
<html
  class="no-js{% if settings.theme_mode_default == "dark" %} dark{% endif %}"
  lang="{{ request.locale.iso_code }}"
  {% if settings.lang_direction contains request.locale.iso_code %}
    dir="rtl"
  {% endif %}
>
  <head>
    


    <!-- Gokwik theme code start -->
    <link rel="dns-prefetch" href="https://pdp.gokwik.co/">
    <link rel="dns-prefetch" href="https://api.gokwik.co">
    {% render 'gokwik' %}


    <!-- Gokwik theme code End -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, height=device-height, minimum-scale=1.0, maximum-scale=1.0"
    >
    <meta name="theme-color" content="">

    {% if product.metafields.custom.canonical != blank %}
      {%- comment -%}Auto-disabled. Yoast plugin is taking care of Meta Tags output.{%- endcomment -%}
      {%- if disabled_by_yoast_seo -%}
        <link rel="canonical" href="{{ product.metafields.custom.canonical }}">
        {%- comment -%} End Yoast auto-disable. {%- endcomment -%}
      {%- endif -%}
    {% else %}
      {%- comment -%}Auto-disabled. Yoast plugin is taking care of Meta Tags output.{%- endcomment -%}
      {%- if disabled_by_yoast_seo -%}
        <link rel="canonical" href="{{ canonical_url }}">
        {%- comment -%} End Yoast auto-disable. {%- endcomment -%}
      {%- endif -%}
    {% endif %}

    {%- if settings.favicon != blank -%}
      <link rel="icon" type="image/png" href="{{ settings.favicon | image_url: width: 32, height: 32 }}">
    {%- endif -%}

    {%- unless settings.type_header_font.system? and settings.type_body_font.system? -%}
      <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
    {%- endunless -%}

    {%- comment -%}Auto-disabled. Yoast plugin is taking care of SEO output.{%- endcomment -%}
    {%- if disabled_by_yoast_seo -%}
      <title>
        {{ page_title }}
        {%- if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif -%}
        {%- if current_page != 1 %} &ndash; Page {{ current_page }}{% endif -%}
        {%- unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless -%}
      </title>
      {%- comment -%} End Yoast auto-disable. {%- endcomment -%}
    {%- endif -%}
    {%- if settings.enable_organization_schema and template.name == 'index' -%}
      {% liquid
        if settings.business_address_1 != null
          assign first_business_addr = settings.business_address_1 | split: ','
        endif
        if settings.business_address_2 != null
          assign second_business_addr = settings.business_address_2 | split: ','
        endif
      %}
      <script type="application/ld+json">
        {
          "@context": "http://schema.org",
          "@type": "{{ settings.site_type }}",
          "name": "{{ settings.business_name }}",
          "description": "{{ settings.business_description }}",
          {% if settings.business_logo != blank %}
          "logo": {{ settings.business_logo | image_url: width: 112 | prepend: "https:" | json }},
          {% endif %}
          "telephone": "{{ settings.business_phone }}",
          "address": [{
            "@type": "PostalAddress",
            "streetAddress": "{{ first_business_addr[0] }}",
            "addressLocality": "{{ first_business_addr[1] }}",
            "addressRegion": "{{ first_business_addr[2] }}",
            "postalCode": "{{ first_business_addr[3] }}",
            "addressCountry": "{{ first_business_addr[4] }}"
          },{
            "streetAddress": "{{ second_business_addr[0] }}",
            "addressLocality": "{{ second_business_addr[1] }}",
            "addressRegion": "{{ second_business_addr[2] }}",
            "postalCode": "{{ second_business_addr[3] }}",
            "addressCountry": "{{ second_business_addr[4] }}"
          }],
          "email": "{{ settings.business_email }}"
        }
      </script>
    {%- endif -%}

    {% if page_description %}
      {%- comment -%}Auto-disabled. Yoast plugin is taking care of Meta Tags output.{%- endcomment -%}
      {%- if disabled_by_yoast_seo -%}
        <meta name="description" content="{{ page_description | escape }}">
        {%- comment -%} End Yoast auto-disable. {%- endcomment -%}
      {%- endif -%}
    {% endif %}

    {% liquid
      render 'meta-tags'
      render 'font-face'
      echo 'theme.css' | asset_url | stylesheet_tag: preload: true
      echo 'review.css' | asset_url | stylesheet_tag: preload: true
      echo 'custom-product.css' | asset_url | stylesheet_tag: preload: true
      echo 'custom.css' | asset_url | stylesheet_tag: preload: true
      echo 'aos.css' | asset_url | stylesheet_tag: preload: true
      render 'css-variables'
      render 'theme-mode-control'
    %}
    {% if request.page_type == 'policy' %}
      {{ 'page.css' | asset_url | stylesheet_tag: preload: true }}
    {% endif %}
    {{ content_for_header }}
    {{ settings.additional_head }}

    <script type="text/javascript">
      (function(c,l,a,r,i,t,y){
          c[a]=c[a]||function(){(c[a].q=c[a].q||[]).push(arguments)};
          t=l.createElement(r);t.async=1;t.src="https://www.clarity.ms/tag/"+i;
          y=l.getElementsByTagName(r)[0];y.parentNode.insertBefore(t,y);
      })(window, document, "clarity", "script", "oqnvmhozde");
    </script>

   


     
    
  {% include 'store-credit-launcher' %}
</head>
  <body
    class="gradient  temp-{{ template.suffix }} temp--{{ template.name }} {% if template.suffix =='khazani-file-upload' %} temp-custom-product {% endif %} pro-{{ product.id }} {% for tag in product.tags %} {% if tag contains "apd" %} apd {% endif %} {% if tag contains "freecm" %} freecm{% endif %} {% if tag contains "offer-ad" %} offer-ad {% endif %} {% if tag contains "dyb-499" %} dyb-499 {% endif %} {% if tag contains "ppd-249" %} ppd-249 {% endif %} {% endfor %}{% if request.path contains 'cart' %}cart-page{% endif %} scrollbar-body overflow-x-hidden text-[{{ settings.text_base_size | times: 0.007875 }}rem] md:text-[{{ settings.text_base_size | times: 0.00875 }}rem]{% if settings.lang_direction contains request.locale.iso_code %} rtl{% endif %}"
    x-data
    :class="$store.xPopup.open && 'overflow-hidden padding-scrollbar'"
  >
    <a
      class="skip-to-content absolute -z-10 button button-solid hidden lg:block pt-2.5 pb-2.5 pl-6 pr-6 lg:pt-3 lg:pb-3 mt-0.5 ml-0.5 leading-normal justify-center cursor-pointer focus-visible:z-60"
      href="#MainContent"
    >
      {{ 'accessibility.skip_to_text' | t }}
    </a>
    {% if template == 'cart' %}
      <style>
        .header, #shopify-section-sections--24139047731497__footer, #x-announcement , #shopify-chat{
          display:none !important;
        }
        
      </style>
    {% endif %}
    {% unless template == 'product.whiff-6-old' %}
    <style>
      .button-product.forever_sticky_atc .stickybar_price_wrapper{
      display:none;
    }
      .add_to_cart_button .hyphen{
      display:none;
    }
    </style>
    {% endunless %}
    


    {% case settings.selected_preset %}
      {% when 'breath' %}
        {% sections 'header-group-breath' %}
      {% when 'swirl' %}
        {% sections 'header-group-swirl' %}
      {% when 'whiff' %}
        {% sections 'header-group-whiff' %}
      {% when 'puff' %}
        {% sections 'header-group-puff' %}
      {% else %}
        {% sections 'header-group' %}
    {% endcase %}
    {% render 'kwikpass-add-button' %}
    <main
      id="MainContent"
      class="content-for-layout focus-none outline-none theme-template-{{ template.name }} theme-template-suffix-{{ template.suffix | default: 'none' }}"
      role="main"
      tabindex="-1"
    >
      {{ content_for_layout }}
    </main>

    {% case settings.selected_preset %}
      {% when 'breath' %}
        {% sections 'footer-group-breath' %}
      {% when 'swirl' %}
        {% sections 'footer-group-swirl' %}
      {% when 'whiff' %}
        {% sections 'footer-group-whiff' %}
      {% when 'puff' %}
        {% sections 'footer-group-puff' %}
      {% else %}
        {% sections 'footer-group' %}
    {% endcase %}

    {% case settings.selected_preset %}
      {% when 'breath' %}
        {% sections 'aside-group-breath' %}
      {% when 'swirl' %}
        {% sections 'aside-group-swirl' %}
      {% when 'whiff' %}
        {% sections 'aside-group-whiff' %}
      {% when 'puff' %}
        {% sections 'aside-group-puff' %}
      {% else %}
        {% sections 'aside-group' %}
    {% endcase %}

    {% if settings.enable_scroll_top_desktop or settings.enable_scroll_top_mobile %}
      {% render 'scroll-to-top' %}
    {% endif %}

    {% section 'sticky_strip' %}

    <ul hidden>
      <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
      <li id="a11y-new-window-message">{{ 'accessibility.link_messages.new_window' | t }}</li>
    </ul>

    <script>
      window.Eurus = {
        sliderScript: '{{ 'splide4.1.3.js' | asset_url }}',
        cart_error: `{{ 'sections.cart.cart_error' | t }}`,
        cart_quantity_error_html: '{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}',
        loadedScript: [],
        cart_add_url: '{{ routes.cart_add_url }}',
        moneyFormat: {{ shop.money_format | json }},
        moneyWithCurrencyFormat: {{ shop.money_with_currency_format | json }},
        currencyCodeEnabled: {{ settings.currency_code_enable | json }}
      };
    </script>

    {%- if request.design_mode -%}
      <script src="{{ 'theme-editor-alpine-custom.js' | asset_url }}" defer></script>
      <script src="{{ 'theme-editor.js' | asset_url }}" defer></script>
    {%- endif -%}
    <script src="{{ 'theme.js' | asset_url }}" defer></script>
    <script src="{{ 'vendors.js' | asset_url }}" defer></script>
    <script src="{{ 'global.js' | asset_url }}" defer></script>
    <script src="{{ 'aos.js' | asset_url }}"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" defer></script>

    <script src="{{ 'custom.js' | asset_url }}" defer></script>

    {% render 'popups' %}

    <script>
      window.addEventListener("load", (event) => {
        deferScriptLoad('instant.page', '{{ 'instant.page.v5.1.1.js' | asset_url }}', () => {}, true);
      });
    </script>

    <!-- window.simplyOtp.settings.forceUpdateProfile = false; -->
    {% section 'otp-section' %}


  
    <script>

         document.addEventListener("DOMContentLoaded", function() {
          let chatButton = document.querySelector("#chat-button"); // Replace with your chat icon ID or class
          if (chatButton) {
              chatButton.addEventListener("click", function() {
                  let script = document.createElement("script");
                  script.src = "https://cdn.shopify.com/extensions/2b5e15c3-eaaa-496f-a806-9f196a59aae2/inbox-1230/assets/shopifyChatV1Widget.js";
                  script.async = true;
                  document.body.appendChild(script);
              }, { once: true }); // Ensures the script loads only once
          }
      });
    </script>
  {% include 'store-credit-launcher' %}
</body>
</html>
